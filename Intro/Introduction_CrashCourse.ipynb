    "            covidDeathsFemale[row[date]] += int(row[covid])\n",
    "        elif row[sex] == \"Male\":\n",
    "            covidDeathsMale[row[date]] += int(row[covid])\n",
    "        else :\n",
    "            print (\"no match:\",row)"
